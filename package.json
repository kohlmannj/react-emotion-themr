    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.6.0",
    "coveralls": "^3.0.2",
    "emotion": "^9.2.6",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
